{
  "extends": [
    "plugin:vue/recommended",
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "vue/max-attributes-per-line": 0,
    "vue/singleline-html-element-content-newline": 0,
    "vue/html-closing-bracket-newline": 0
  },
  "settings": {
    "import/resolver": "nuxt"
  }
}
